<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <padding
                android:bottom="26dp"
                android:left="26dp"
                android:right="26dp"
                android:top="26dp" />
            <size
                android:width="128dp"
                android:height="128dp" />
            <gradient
                android:angle="135"
                android:endColor="@color/md_green_A700"
                android:startColor="@color/md_blue_A200" />
        </shape>
    </item>
    <item>
        <rotate
            android:drawable="@drawable/retro_music_logo"
            android:fromDegrees="0"
            android:toDegrees="0" />
    </item>
</layer-list>